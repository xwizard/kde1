msgstr ""
"Kelc'h\n"
"\n"
"a dres ur c'helc'h"

#: kiconedit.cpp:895
msgid ""
"Filled circle\n"
"\n"
"Draw a filled circle"
msgstr ""
"Kelc'h leun\n"
"\n"
"a dres ur c'helc'h leun"

#: kiconedit.cpp:898
msgid ""
"Ellipse\n"
