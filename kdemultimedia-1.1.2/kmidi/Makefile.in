uninstall:
	rm -f $(TIMID_DIR)/patch/acpiano.pat
	rm -f $(TIMID_DIR)/patch/nylongt2.pat

messages:
	$(XGETTEXT) -C -ki18n -x $(includedir)/kde.pot $(kmidi_SOURCES) && mv messages.po ../po/kmidi.pot

# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:


$(srcdir)/kmidi.cpp: kmidi.moc
kmidi.moc: $(srcdir)/kmidi.h
	$(MOC) $(srcdir)/kmidi.h -o kmidi.moc

$(srcdir)/playlist.cpp: playlist.moc
playlist.moc: $(srcdir)/playlist.h
	$(MOC) $(srcdir)/playlist.h -o playlist.moc

$(srcdir)/bwlednum.cpp: bwlednum.moc
bwlednum.moc: $(srcdir)/bwlednum.h
	$(MOC) $(srcdir)/bwlednum.h -o bwlednum.moc

$(srcdir)/configdlg.cpp: configdlg.moc
configdlg.moc: $(srcdir)/configdlg.h
	$(MOC) $(srcdir)/configdlg.h -o configdlg.moc

$(srcdir)/log.cpp: log.moc
log.moc: $(srcdir)/log.h
	$(MOC) $(srcdir)/log.h -o log.moc

distclean-metasources:
