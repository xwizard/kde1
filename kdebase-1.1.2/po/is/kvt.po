#: main.C:1478
msgid "Permitted arguments are:\n"
msgstr "Lögleg viðföng eru:\n"

#: main.C:1479
msgid ""
"-e <command> <arg> ...    execute command with ars - must be last argument\n"
msgstr ""
"-e <skipun> <viðföng> ... framkvæma skipun með viðföngum - alltaf aftast!\n"

#: main.C:1480
msgid "-display <name>           specify the display (server)\n"
msgstr "-display <nafn>           tiltaka X-skjá (þjón)\n"

#: main.C:1481
msgid "-vt_geometry <spec>       the initial vt window geometry\n"
msgstr "-vt_gemoetry <stilling>   upphafsstillingar fyrir vt-gluggann\n"

#: main.C:1482
msgid "-print-pipe <name>        specify pipe for vt100 printer\n"
msgstr "-print-pipe <name>        tiltaka pípu fyrir vt100-prentara\n"

#: main.C:1483
msgid "-vt_bg <colour>           background color\n"
msgstr "-vt_bh <litur>            bakgrunnslitur\n"

#: main.C:1484
msgid "-vt_fg <colour>           foreground color\n"
msgstr "-vt_fg <litur>            forgrunnslitur\n"

#: main.C:1485
msgid "-vt_font <fontname>       normal font\n"
msgstr "-vt_font <leturnafn>      venjulega letrið\n"

#: main.C:1486
msgid "-no_font_hack             turn off Qt font bug workaround\n"
msgstr "-no_font_hack             ekki fara framhjá villum í Qt\n"

#: main.C:1487
msgid "-vt_size <size>           tiny, small, normal, large, huge\n"
msgstr ""
"-vt_size <stærð>          leturstærð: tiny, small, normal, large, huge\n"

#: main.C:1488
msgid "-linux                    set up kvt to mimic linux-console\n"
msgstr "-linux                    láta kvt herma eftir Linux-textaham\n"

#: main.C:1489
msgid "-no_menubar               hide the menubar\n"
msgstr "-no_menubar               fela valmyndastiku\n"

#: main.C:1490
msgid "-no_scrollbar             hide the scrollbar\n"
msgstr "-no_scrollbar             fela rennislá\n"

#: main.C:1491
msgid ""
"-T <text>                 text in window titlebar \n"
"                          (Obsolete. Use -caption instead)\n"
msgstr ""
"-T <texti>                texti í gluggatitli. (Úrelt, notið -caption)\n"

#: main.C:1493
msgid "-tn <TERM>                Terminal type. Default is xterm.\n"
msgstr "-tn <TERM>                Skjágerð. Xterm er sjálfgefið.\n"

#: main.C:1494
msgid "-C                        Capture system console message\n"
msgstr "-C                        grípa kerfisskjáboð\n"

#: main.C:1495
msgid "-n <text>                 name in icon or icon window\n"
msgstr "-n <text>                 nafn í táknmynd eða táknmyndarglugga\n"

#: main.C:1496
msgid ""
"-7                        run in 7 bit mode\n"
"-8                        run in 8 bit mode\n"
msgstr ""
"-7                        keyra í 7 bita ham\n"
"-8                        keyra í 8 bita ham\n"

#: main.C:1498
msgid ""
"-ls                       initiate kvt's shell as a login shell\n"
"-ls-                      initiate kvt's shell as a non-login shell "
"(default)\n"
msgstr ""
"-ls                       opna innskráningarskel í kvt\n"
"-ls-                      opna venjulega skel í kvt (sjálfgefið)\n"

#: main.C:1500
msgid ""
"-meta <arg>               handle Meta with ESCAPE prefix, 8THBIT set, or "
"ignore\n"
msgstr ""
"-meta <arg>               höndla Meta með ESCAPE forskeyti,\n"
"                          setja áttunda bita á, eða henda\n"

#: main.C:1501
msgid "-sl <number>              save number lines in scroll-back buffer\n"
msgstr "-sl <fjöldi>              geyma tiltekinn fjölda lína í biðminni\n"

#: main.C:1502
msgid ""
"-pageup <keysym>          use hot key alt-keysym to scroll up through the "
"buffer\n"
"-pagedown <keysym>        use hot key alt-keysym to scroll down through "
"buffer\n"
msgstr ""
"-pageup <lyklatákn>       nota alt-lyklatákn til að renna aftur í biðminni\n"
"-pagedown <lyklatákn>     nota alt-lyklatákn til að renna fram í biðminni\n"

#: main.C:1506
msgid ""
"-grk9\t\tgreek kbd = ELOT 928 (default)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
msgstr ""
"-grk9                     grískt lyklaborð = ELOT 928 (sjálfgefið)\n"
"-grk4                     grískt lyklaborð = IBM 437\n"
