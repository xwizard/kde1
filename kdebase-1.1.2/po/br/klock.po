#: main.cpp:664
#, c-format
msgid ""
"Usage: %s -install|-setup|-test|-desc|-preview wid|-inroot\n"
"       [-corners xxxx] [-delay num] [-lock] [-allow-root] [-nice num]\n"
msgstr ""
"Arveriadur : %s -install|-setup|-test|-desc|-preview led|-inroot\n"
"       [-corners xxxx] [-delay niv] [-lock] [-allow-root] [-nice niv]\n"

#: main.cpp:667
msgid ""
"  -corners xxxx     Placing cursor in corner performs action:\n"
"                     x = i  no action (ignore)\n"
"                     x = s  save screen\n"
"                     x = l  lock screen\n"
"                    order: top-left, top-right, bottom-left, bottom-right\n"
"  -delay num        Amount of idle time before screen saver\n"
"                     starts  (default 10min)\n"
"  -desc             Print the screen saver's description to stdout\n"
"  -install          Install screen saver\n"
"  -lock             Require password to stop screen saver\n"
"  -allow-root       Accept root password to unlock\n"
"  -nice num         Run with specified nice value\n"
"\n"
"  -preview wid      Run in the specified XWindow\n"
"  -inroot           Run in the root window\n"
"  -setup            Setup screen saver\n"
"  -test             Invoke the screen saver immediately\n"
msgstr ""
"  -corners xxxx     Lakaat ar reti er c'horn a seven  an obererezh :\n"
"                     x = i  hini ebet (na ober van)\n"
"                     x = s  damantiñ ar skramm\n"
"                     x = l  prennañ ar skramm\n"
"                    urzh : gorre-kleiz, gorre-dehou, traoñ-kleiz, "
"traoñ-dehou\n"
"  -delay niv       gedvezh araok loc''hañ an damanter\n"
"                      skramm  (dre ziouer 10mun)\n"
"  -desc             moulañ deskrivadur an damanter skramm war stdout\n"
"  -install          staliañ an damanter skramm\n"
"  -lock             goulenn an tremenger evit ehanañ an damanter skramm\n"
"  -allow-root       Aotreañ root da dibrennañ\n"
"  -nice num         a seven gant ar gwerzh nice roet\n"
"\n"
"  -preview wid      seveniñ er prenestr XWindow resisaet\n"
"  -inroot           seveniñ er prenestr gwrizienn\n"
"  -setup            kefluniañ an damanter skramm\n"
"  -test              loc'h an damanter skramm diouzhtu\n"

