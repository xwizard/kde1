#: main.cpp:664
#, c-format
msgid ""
"Usage: %s -install|-setup|-test|-desc|-preview wid|-inroot\n"
"       [-corners xxxx] [-delay num] [-lock] [-allow-root] [-nice num]\n"
msgstr ""
"Utilização: %s -install|-setup|-test|-desc|-preview wid|-inroot\n"
"       [-corners xxxx] [-delay num] [-lock] [-allow-root] [-nice num]\n"

#: main.cpp:667
msgid ""
"  -corners xxxx     Placing cursor in corner performs action:\n"
"                     x = i  no action (ignore)\n"
"                     x = s  save screen\n"
"                     x = l  lock screen\n"
"                    order: top-left, top-right, bottom-left, bottom-right\n"
"  -delay num        Amount of idle time before screen saver\n"
"                     starts  (default 10min)\n"
"  -desc             Print the screen saver's description to stdout\n"
"  -install          Install screen saver\n"
"  -lock             Require password to stop screen saver\n"
"  -allow-root       Accept root password to unlock\n"
"  -nice num         Run with specified nice value\n"
"\n"
"  -preview wid      Run in the specified XWindow\n"
"  -inroot           Run in the root window\n"
"  -setup            Setup screen saver\n"
"  -test             Invoke the screen saver immediately\n"
msgstr ""
"  -corners xxxx     Colocar o cursor na tela produz a seguinte ação:\n"
"                     x = i  nenhuma ação (ignorar)\n"
"                     x = s  proteger tela\n"
"                     x = l  bloquear tela\n"
"                    ordem: acima-esquerdo, acima-direito, embaixo-esquerdo, "
"embaixo-direito\n"
"  -delay num        Quantidade de tempo parado antes de começar a\n"
"                     proteção da tela  (por padrão 10min)\n"
"  -desc             Imprime a descrição para o stdout\n"
"  -install          Instalar a proteção de tela\n"
"  -lock             Requerer a senha para parar a proteção de tela\n"
"  -nice num         Executar com o valor especificado de nice\n"
"  -allow-root       Aceita senha de superusuário para destrancar\n"
"\n"
"  -preview wid      Executar na X Window especificada\n"
"  -inroot           Executar na janela de root\n"
"  -setup            Configurar proteção de tela\n"
"  -test             Inicializar a proteção de tela imediatamente\n"

