#: main.cpp:664
#, c-format
msgid ""
"Usage: %s -install|-setup|-test|-desc|-preview wid|-inroot\n"
"       [-corners xxxx] [-delay num] [-lock] [-allow-root] [-nice num]\n"
msgstr ""
"Modo de uso: %s -install|-setup|-test|-desc|-preview wid|-inroot\n"
"   [-corners xxxx] [-delay num] [-lock] [-allow-root] [-nice num]\n"

#: main.cpp:667
msgid ""
"  -corners xxxx     Placing cursor in corner performs action:\n"
"                     x = i  no action (ignore)\n"
"                     x = s  save screen\n"
"                     x = l  lock screen\n"
"                    order: top-left, top-right, bottom-left, bottom-right\n"
"  -delay num        Amount of idle time before screen saver\n"
"                     starts  (default 10min)\n"
"  -desc             Print the screen saver's description to stdout\n"
"  -install          Install screen saver\n"
"  -lock             Require password to stop screen saver\n"
"  -allow-root       Accept root password to unlock\n"
"  -nice num         Run with specified nice value\n"
"\n"
"  -preview wid      Run in the specified XWindow\n"
"  -inroot           Run in the root window\n"
"  -setup            Setup screen saver\n"
"  -test             Invoke the screen saver immediately\n"
msgstr ""
"  -corners xxxx     Situando el cursor en una esquina se realiza:\n"
"                     x = i  nada (ignore)\n"
"                     x = s  guarda la pantalla\n"
"                     x = l  bloquea la pantalla\n"
"                    orden: arr.-izq., arr.-der., abajo-izq., abajo-der.\n"
"  -delay num        Número de minutos de inactividad antes de que el\n"
"                    salvapantallas se active (predeterminado 10min)\n"
"  -desc             Imprimir la descripción del salvapantallas en stdout\n"
"  -install          Instalar el salvapantallas\n"
"  -lock             Requiere contraseña para parar el salvapantallas\n"
"  -nice num         Ejecutar con el valor específico 'nice'\n"
"\n"
"  -preview wid      Ejecutar en una ventana específica\n"
"  -inroot           Ejecutar en el fondo de la pantalla\n"
"  -setup            Configurar el salvapantallas\n"
"  -test             Ejecutar el salvapantallas inmediatamente\n"

