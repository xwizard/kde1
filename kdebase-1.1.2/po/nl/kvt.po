#: main.C:1478
msgid "Permitted arguments are:\n"
msgstr "Toegestane argumenten zijn:\n"

# FIXME: 'ars' should be 'args'
#: main.C:1479
msgid ""
"-e <command> <arg> ...    execute command with ars - must be last argument\n"
msgstr ""
"-e <commando> <arg> ...    voer commando uit met args, met '-' als laatste\n"

#: main.C:1480
msgid "-display <name>           specify the display (server)\n"
msgstr "-display <naam>           specificeer de display (server)\n"

#: main.C:1481
msgid "-vt_geometry <spec>       the initial vt window geometry\n"
msgstr "-vt_geometry <spec>       de oorspronkelijke vt venster geometrie\n"

#: main.C:1482
msgid "-print-pipe <name>        specify pipe for vt100 printer\n"
msgstr "-print-piper <naam>      speciceer pipe voor vt100 printer\n"

#: main.C:1483
msgid "-vt_bg <colour>           background color\n"
msgstr "-vt_bg <kleur>            achtergrond kleur\n"

#: main.C:1484
msgid "-vt_fg <colour>           foreground color\n"
msgstr "-vt_fg <kleur>            voorgrond kleur\n"

#: main.C:1485
msgid "-vt_font <fontname>       normal font\n"
msgstr "-vt_font <lettertypenaam> normaal lettertype\n"

#: main.C:1486
msgid "-no_font_hack             turn off Qt font bug workaround\n"
msgstr ""

#: main.C:1487
msgid "-vt_size <size>           tiny, small, normal, large, huge\n"
msgstr "-vt_size <grootte>        zeer klein,klein,normaal,groot,zeer groot\n"

#: main.C:1488
msgid "-linux                    set up kvt to mimic linux-console\n"
msgstr "-linux                    stel kvt in als linux-console\n"

#: main.C:1489
msgid "-no_menubar               hide the menubar\n"
msgstr "-no_menubar               verberg de menubalk\n"

#: main.C:1490
msgid "-no_scrollbar             hide the scrollbar\n"
msgstr "-no_scrollbar             verberg de schijfbalk\n"

#: main.C:1491
msgid ""
"-T <text>                 text in window titlebar \n"
"                          (Obsolete. Use -caption instead)\n"
msgstr ""
"-T <tekst>                tekst in de titelbalk van het venster \n"
"                          (Gedateerd. Gebruik -caption in plaats hiervan)\n"

#: main.C:1493
msgid "-tn <TERM>                Terminal type. Default is xterm.\n"
msgstr "-tn <TERM>                Terminal type. Standard is xterm.\n"

#: main.C:1494
msgid "-C                        Capture system console message\n"
msgstr "-C                        Onderschep systeem console boodschap\n"

#: main.C:1495
msgid "-n <text>                 name in icon or icon window\n"
msgstr "-n <tekst>                naam in pictogram of pictogram venster\n"

#: main.C:1496
msgid ""
"-7                        run in 7 bit mode\n"
"-8                        run in 8 bit mode\n"
msgstr ""
"-7                        voer uit in 7 bit mode\n"
"-8                        voer uit in 8 bit mode\n"

#: main.C:1498
msgid ""
"-ls                       initiate kvt's shell as a login shell\n"
"-ls-                      initiate kvt's shell as a non-login shell "
"(default)\n"
msgstr ""
"-ls                       start kvt's shell als een login shell\n"
"-ls-                      start kvt's shell als een non-login shell "
"(standaard)\n"

#: main.C:1500
msgid ""
"-meta <arg>               handle Meta with ESCAPE prefix, 8THBIT set, or "
"ignore\n"
msgstr ""
"-meta <arg>               doe Meta met ESCAPE prefix, 8THBIT ingesteld, of "
"negeer\n"

#: main.C:1501
msgid "-sl <number>              save number lines in scroll-back buffer\n"
msgstr "-sl <nummer>              sla aantal regels in terugschuifbuffer op\n"

#: main.C:1502
msgid ""
"-pageup <keysym>          use hot key alt-keysym to scroll up through the "
"buffer\n"
"-pagedown <keysym>        use hot key alt-keysym to scroll down through "
"buffer\n"
msgstr ""
"-pageup <keysym>          gebruik toets alt-keysym om omhoog te schuiven "
"door de buffer\n"
"-pagedown <keysym>        gebruik toets alt-keysym om omlaag te schuiven "
"door de buffer\n"

#: main.C:1506
msgid ""
"-grk9\t\tgreek kbd = ELOT 928 (default)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
msgstr ""
"-grk9\t\tgriekse toetsenbord is ELOT 928 (standaard)\n"
"-grk4\t\tgriekse toetsenbord is IBM 437\n"

