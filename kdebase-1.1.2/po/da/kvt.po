#: main.C:1478
msgid "Permitted arguments are:\n"
msgstr "Tilladte argumenter er:\n"

#: main.C:1479
msgid ""
"-e <command> <arg> ...    execute command with ars - must be last argument\n"
msgstr ""
"-e <command> <arg> ...    udfør kommando med args - skal være sidste\n"
" argument\n"

#: main.C:1480
msgid "-display <name>           specify the display (server)\n"
msgstr "-display <name>           specificer hvilket display (server)\n"

#: main.C:1481
msgid "-vt_geometry <spec>       the initial vt window geometry\n"
msgstr "-vt_geometry <spec>        vt vinduesgeometri til at begynde med\n"

#: main.C:1482
msgid "-print-pipe <name>        specify pipe for vt100 printer\n"
msgstr "-print-pipe <name>        specificer  pipe for vt100 printeren\n"

#: main.C:1483
msgid "-vt_bg <colour>           background color\n"
msgstr "-vt_bg <colour>           baggrundsfarve\n"

#: main.C:1484
msgid "-vt_fg <colour>           foreground color\n"
msgstr "-vt_fg <colour>           forgrundsfarve\n"

#: main.C:1485
msgid "-vt_font <fontname>       normal font\n"
msgstr "-vt_font <fontname>       normalskrift\n"

#: main.C:1486
msgid "-no_font_hack             turn off Qt font bug workaround\n"
msgstr ""
"-no_font_hack             benyt ikke Qt's omgåelse af skrifttypeproblem\n"

#: main.C:1487
msgid "-vt_size <size>           tiny, small, normal, large, huge\n"
msgstr "-vt_size <size>           tiny, small, normal, large, huge\n"

#: main.C:1488
msgid "-linux                    set up kvt to mimic linux-console\n"
msgstr "-linux                    sæt  kvt til at efterligne linux-konsol\n"

#: main.C:1489
msgid "-no_menubar               hide the menubar\n"
msgstr "-no_menubar               skjul menubjælken\n"

#: main.C:1490
msgid "-no_scrollbar             hide the scrollbar\n"
msgstr "-no_scrollbar             skjul rullebjælken\n"

#: main.C:1491
msgid ""
"-T <text>                 text in window titlebar \n"
"                          (Obsolete. Use -caption instead)\n"
msgstr ""
"-T <tekst>                 tekst i winduets titelbjælke \n"
"                          (Forældet. Brug -caption i stedetfor)\n"

#: main.C:1493
msgid "-tn <TERM>                Terminal type. Default is xterm.\n"
msgstr "-tn <TERM>                Terminal type. Standard er xterm.\n"

#: main.C:1494
msgid "-C                        Capture system console message\n"
msgstr "-C                        Vis systemkonsolbeskedder\n"

#: main.C:1495
msgid "-n <text>                 name in icon or icon window\n"
msgstr "-n <text>                 navn i ikon eller ikon vindue\n"

#: main.C:1496
msgid ""
"-7                        run in 7 bit mode\n"
"-8                        run in 8 bit mode\n"
msgstr ""
"-7                        kør i 7 bit tilstand\n"
"-8                        kør in 8 bit tilstand\n"

#: main.C:1498
msgid ""
"-ls                       initiate kvt's shell as a login shell\n"
"-ls-                      initiate kvt's shell as a non-login shell "
"(default)\n"
msgstr ""
"-ls                       begynd kvt's shell som en log ind shell\n"
"-ls-                      begynd kvt's shell som en  ikke-log ind shell "
"(standard)\n"

#: main.C:1500
msgid ""
"-meta <arg>               handle Meta with ESCAPE prefix, 8THBIT set, or "
"ignore\n"
msgstr ""
"-meta <arg>               behandl Meta med ESCAPE prefix, 8THBIT sat, eller "
"ignorer\n"

#: main.C:1501
msgid "-sl <number>              save number lines in scroll-back buffer\n"
msgstr "-sl <antal>                 gem antal linier i rul-tilbage bufferen\n"

#: main.C:1502
msgid ""
"-pageup <keysym>          use hot key alt-keysym to scroll up through the "
"buffer\n"
"-pagedown <keysym>        use hot key alt-keysym to scroll down through "
"buffer\n"
msgstr ""
"-pageup <keysym>          brug genvejen alt-tastesymbol til at rulle op "
"gennem bufferen\n"
"-pagedown <keysym>        brug genvejen alt-tastesymbol til at rulle ned "
"gennem bufferen\n"
"buffer\n"

#: main.C:1506
msgid ""
"-grk9\t\tgreek kbd = ELOT 928 (default)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
msgstr ""
"-grk9\t\tgræsk er = ELOT 928 (standard)\n"
"-grk4\t\tgræsk er = IBM 437\n"
