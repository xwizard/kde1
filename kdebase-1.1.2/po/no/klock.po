#: main.cpp:664
#, c-format
msgid ""
"Usage: %s -install|-setup|-test|-desc|-preview wid|-inroot\n"
"       [-corners xxxx] [-delay num] [-lock] [-allow-root] [-nice num]\n"
msgstr ""
"Bruk: %s -install|-setup|-test|-desc|-preview wid|-inroot\n"
"       [-corners xxxx] [-delay num] [-lock] [-allow-root] [-nice num]\n"

#: main.cpp:667
msgid ""
"  -corners xxxx     Placing cursor in corner performs action:\n"
"                     x = i  no action (ignore)\n"
"                     x = s  save screen\n"
"                     x = l  lock screen\n"
"                    order: top-left, top-right, bottom-left, bottom-right\n"
"  -delay num        Amount of idle time before screen saver\n"
"                     starts  (default 10min)\n"
"  -desc             Print the screen saver's description to stdout\n"
"  -install          Install screen saver\n"
"  -lock             Require password to stop screen saver\n"
"  -allow-root       Accept root password to unlock\n"
"  -nice num         Run with specified nice value\n"
"\n"
"  -preview wid      Run in the specified XWindow\n"
"  -inroot           Run in the root window\n"
"  -setup            Setup screen saver\n"
"  -test             Invoke the screen saver immediately\n"
msgstr ""
"  -corners xxxx     Plassering av cursor i hjørne utfører handling:\n"
"                     x = i  ingen handling (ignorer)\n"
"                     x = s  lagre skjerm\n"
"                     x = l  låse skjerm\n"
"                    rekkefølge: topp-venstre, topp-høyre, bunn-venstre, "
"bunn-høyre\n"
"  -delay num        Tid PC ikke er i bruk før skjermsparer\n"
"                     starter  (standard 10min)\n"
"  -desc             Skriv skjermsparerens beskrivelse til stdout\n"
"  -install          Installer skjermsparer\n"
"  -lock             Krever passord for å stoppe skjermsparer\n"
"  -allow-root       Akseptere root-passord for å låse opp\n"
"  -nice num         Kjør med spesifisert fin verdi\n"
"\n"
"  -preview wid      Kjør i det spesifiserte XWindow\n"
"  -inroot           Kjør i root window\n"
"  -setup            Skjermsparer oppsett\n"
"  -test             Ta i bruk skjermspareren øyeblikkelig\n"

