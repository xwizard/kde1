#: main.C:1478
msgid "Permitted arguments are:\n"
msgstr "Els arguments permesos són:\n"

#: main.C:1479
msgid ""
"-e <command> <arg> ...    execute command with ars - must be last argument\n"
msgstr ""
"-e <comanda> <arg> ...    executa comanda amb arg - ha d'estar al final\n"

#: main.C:1480
msgid "-display <name>           specify the display (server)\n"
msgstr "-display <nom>            especifica la pantalla (servidor)\n"

#: main.C:1481
msgid "-vt_geometry <spec>       the initial vt window geometry\n"
msgstr "-vt_geometry <espec>      la geometria inicial de la finestra vt\n"

#: main.C:1482
msgid "-print-pipe <name>        specify pipe for vt100 printer\n"
msgstr "-print-pipe <nom>         especifica el tub per la impressora vt100\n"

#: main.C:1483
msgid "-vt_bg <colour>           background color\n"
msgstr "-vt_bg <color>            color de fons\n"

#: main.C:1484
msgid "-vt_fg <colour>           foreground color\n"
msgstr "-vt_fg <color>            color de primer pla\n"

#: main.C:1485
msgid "-vt_font <fontname>       normal font\n"
msgstr "-vt_font <nomfont>        font normal\n"

#: main.C:1486
msgid "-no_font_hack             turn off Qt font bug workaround\n"
msgstr "-no_font_hack             desactiva la solució a l'error de font Qt\n"

#: main.C:1487
msgid "-vt_size <size>           tiny, small, normal, large, huge\n"
msgstr "-vt_size <mida>           minúscula, petita, mitjana, gran, enorme\n"

#: main.C:1488
msgid "-linux                    set up kvt to mimic linux-console\n"
msgstr "-linux                    arranja kvt per imitar la consola linux\n"

#: main.C:1489
msgid "-no_menubar               hide the menubar\n"
msgstr "-no_menubar               oculta la barra de menú\n"

#: main.C:1490
msgid "-no_scrollbar             hide the scrollbar\n"
msgstr "-no_scrollbar             oculta la barra de desplaçament\n"

#: main.C:1491
msgid ""
"-T <text>                 text in window titlebar \n"
"                          (Obsolete. Use -caption instead)\n"
msgstr ""
"-T <text>                 text del títol de finestra \n"
"                          (Obsolet. Useu l'opció - caption)\n"

#: main.C:1493
msgid "-tn <TERM>                Terminal type. Default is xterm.\n"
msgstr "-tn <TERM>                Tipus de terminal. Per omissió és xterm.\n"

#: main.C:1494
msgid "-C                        Capture system console message\n"
msgstr "-C                        Captura els missatges de la consola\n"

#: main.C:1495
msgid "-n <text>                 name in icon or icon window\n"
msgstr "-n <text>                 nom de la icona o la finestra de la icona\n"

#: main.C:1496
msgid ""
"-7                        run in 7 bit mode\n"
"-8                        run in 8 bit mode\n"
msgstr ""
"-7                        executa en mode 7 bits\n"
"-8                        executa en mode 8 bits\n"

#: main.C:1498
msgid ""
"-ls                       initiate kvt's shell as a login shell\n"
"-ls-                      initiate kvt's shell as a non-login shell "
"(default)\n"
msgstr ""
"-ls                       inicia kvt amb un nou login\n"
"-ls-                      inicia kvt sense un nou login (omissió)\n"

#: main.C:1500
msgid ""
"-meta <arg>               handle Meta with ESCAPE prefix, 8THBIT set, or "
"ignore\n"
msgstr ""
"-meta <arg>               manega Meta amb prefix ESCAPE, 8THBIT fix, o "
"ignora\n"

#: main.C:1501
msgid "-sl <number>              save number lines in scroll-back buffer\n"
msgstr ""
"-sl <nombre>              desa nombre línies a l'àrea de desplaçament "
"enrera\n"

#: main.C:1502
msgid ""
"-pageup <keysym>          use hot key alt-keysym to scroll up through the "
"buffer\n"
"-pagedown <keysym>        use hot key alt-keysym to scroll down through "
"buffer\n"
msgstr ""
"-pageup <keysym>          usa la tecla alt-keysym per desplaçar-se amunt\n"
"-pagedown <keysym>        usa la tecla alt-keysym per desplaçar-se avall\n"

#: main.C:1506
msgid ""
"-grk9\t\tgreek kbd = ELOT 928 (default)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
msgstr ""
"-grk9\t\tteclat grec = ELOT 928 (omissió)\n"
"-grk4\t\tteclat grec = IBM 437\n"
