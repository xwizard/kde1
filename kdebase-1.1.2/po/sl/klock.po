#: main.cpp:572
msgid ""
"Usage: %1 -install|-setup|-test|-desc|-preview wid|-inroot\n"
"       [-corners xxxx] [-delay num] [-lock] [-allow-root] [-nice num]\n"
msgstr ""
"Uporaba: %1 -install|-setup|-test|-desc|-preview okno|-inroot\n"
"         [-corners xxxx] [-delay ¹tev] [-lock] [-allow-root] [-nice ¹tev]\n"

#: main.cpp:575
msgid ""
"  -corners xxxx     Placing cursor in corner performs action:\n"
"                     x = i  no action (ignore)\n"
"                     x = s  save screen\n"
"                     x = l  lock screen\n"
"                    order: top-left, top-right, bottom-left, bottom-right\n"
"  -delay num        Amount of idle time before screen saver\n"
"                     starts  (default 10min)\n"
"  -desc             Print the screen saver's description to stdout\n"
"  -install          Install screen saver\n"
"  -lock             Require password to stop screen saver\n"
"  -allow-root       Accept root password to unlock\n"
"  -nice num         Run with specified nice value\n"
"\n"
"  -preview wid      Run in the specified XWindow\n"
"  -inroot           Run in the root window\n"
"  -setup            Setup screen saver\n"
"  -test             Invoke the screen saver immediately\n"
msgstr ""
"  -corners xxxx     Postavitev kazalca v kot izvede dejanje:\n"
"                     x = i  ni dejanja (ignoriraj)\n"
"                     x = s  shrani zaslon\n"
"                     x = l  zakleni zaslon\n"
"                    vrstni red: zgoraj-levo, zgoraj-desno, spodaj-levo,\n"
"                                 spodaj-desno\n"
"  -delay ¹tev       Èas mirovanja preden se vklopi osve¾evalec zaslona\n"
"                     (privzeeto 10min)\n"
"  -desc             Izpi¹i opis osve¾evalca zaslona na standardni izhod\n"
"  -install          Namesti osve¾evalec zaslona\n"
"  -lock             Zahtevaj geslo za zaustavitev osve¾evalca zaslona\n"
"  -allow-root       Sprejmi geslo uporabnika root za odklenitev\n"
"  -nice num         Teèi z doloèeno nice vrednostjo\n"
"\n"
"  -preview okno     Teèi v doloèenem XOknu\n"
"  -inroot           Teèi v korenskem oknu\n"
"  -setup            Nastavi osve¾evalec zaslona\n"
"  -test             Takoj za¾eni osve¾evalec zaslona\n"

