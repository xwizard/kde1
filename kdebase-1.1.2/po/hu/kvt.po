#: main.C:1478
msgid "Permitted arguments are:\n"
msgstr "Engedélyezett paraméterek:\n"

#: main.C:1479
msgid ""
"-e <command> <arg> ...    execute command with ars - must be last argument\n"
msgstr ""
"-e <parancs> <par> ...    parancs futtatása parméterrel - utolsó paraméter kell legyen\n"

#: main.C:1480
msgid "-display <name>           specify the display (server)\n"
msgstr "-display <név>            kijelzõ (kiszolgáló) megadása\n"

#: main.C:1481
msgid "-vt_geometry <spec>       the initial vt window geometry\n"
msgstr "-vt_geometry <méret>      kezdõ ablak méret\n"

#: main.C:1482
msgid "-print-pipe <name>        specify pipe for vt100 printer\n"
msgstr "-print-pipe <név>         pipe megadása vt100 printerhez\n"

#: main.C:1483
msgid "-vt_bg <colour>           background color\n"
msgstr "-vt_bg <szín>             háttér szín\n"

#: main.C:1484
msgid "-vt_fg <colour>           foreground color\n"
msgstr "-vt_fg <szín>             elõtér szín\n"

#: main.C:1485
msgid "-vt_font <fontname>       normal font\n"
msgstr "-vt_font <betûtípus>      alap betûtípus\n"

#: main.C:1486
msgid "-no_font_hack             turn off Qt font bug workaround\n"
msgstr "-no_font_hack             Qt betûtípus hiba kikerülés kikapcsolása\n"

#: main.C:1487
msgid "-vt_size <size>           tiny, small, normal, large, huge\n"
msgstr "-vt_size <méret>          tiny, small, normal, large, huge\n"

#: main.C:1488
msgid "-linux                    set up kvt to mimic linux-console\n"
msgstr "-linux                    linux-konzol utánzás beállítása\n"

#: main.C:1489
msgid "-no_menubar               hide the menubar\n"
msgstr "-no_menubar               menüsor elrejtése\n"

#: main.C:1490
msgid "-no_scrollbar             hide the scrollbar\n"
msgstr "-no_scrollbar             gördítõsor elrejtése\n"

#: main.C:1491
msgid ""
"-T <text>                 text in window titlebar \n"
"                          (Obsolete. Use -caption instead)\n"
msgstr ""
"-T <szöveg>               szöveg a címsorban \n"
"                          (Régimódi. Használd a -caption -t inkább)\n"

#: main.C:1493
msgid "-tn <TERM>                Terminal type. Default is xterm.\n"
msgstr "-tn <TERM>                Terminál típus. Alapértelmezett: xterm.\n"

#: main.C:1494
msgid "-C                        Capture system console message\n"
msgstr "-C                        Rendszer konzol üzenetek elfogása\n"

#: main.C:1495
msgid "-n <text>                 name in icon or icon window\n"
msgstr "-n <szöveg>               név ikon vagy ikon ablak alakban\n"

#: main.C:1496
msgid ""
"-7                        run in 7 bit mode\n"
"-8                        run in 8 bit mode\n"
msgstr ""
"-7                        7 bites mód\n"
"-8                        8 bites mód\n"

#: main.C:1498
msgid ""
"-ls                       initiate kvt's shell as a login shell\n"
"-ls-                      initiate kvt's shell as a non-login shell "
"(default)\n"
msgstr ""
"-ls                       kvt, mint bejelentkezõ parancsértelmezõ\n"
"-ls-                      kvt, mint nem-bejelentkezõ parancsértelmezõ \n"
"(alapért.)\n"

#: main.C:1500
msgid ""
"-meta <arg>               handle Meta with ESCAPE prefix, 8THBIT set, or "
"ignore\n"
msgstr ""
"-meta <par>               Meta kezelés ESCAPE elõtaggal, 8THBIT, vagy \n"
"ignore\n"

#: main.C:1501
msgid "-sl <number>              save number lines in scroll-back buffer\n"
msgstr "-sl <szám>                ennyi sort mentsen a gördítõ pufferbe\n"

#: main.C:1502
msgid ""
"-pageup <keysym>          use hot key alt-keysym to scroll up through the "
"buffer\n"
"-pagedown <keysym>        use hot key alt-keysym to scroll down through "
"buffer\n"
msgstr ""
"-pageup <keysym>          alt-keysym használata a puffer \n"
"felgörgetéséhez\n"
"-pagedown <keysym>        alt-keysym használata a puffer \n"
"legörgetéséhez\n"

# main.C:954
#: main.C:1506
msgid ""
"-grk9\t\tgreek kbd = ELOT 928 (default)\n"
"-grk4\t\tgreek kbd = IBM 437\n"
msgstr ""
"-grk9\t\tgörög kbd = ELOT 928 (alapértelmezett)\n"
"-grk4\t\tgörög kbd = IBM 437\n"
