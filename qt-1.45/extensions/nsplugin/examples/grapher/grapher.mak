moc: $(SRCMOC)

tmake: grapher.mak

grapher.mak: grapher.pro
	tmake grapher.pro -nodepend -o grapher.mak

clean:
	-del grapher.obj
	-del grapher.moc
	-del $(TARGET)

####### Compile

grapher.obj: grapher.cpp \
		grapher.moc

